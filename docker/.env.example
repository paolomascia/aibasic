# AIbasic Docker Environment Variables
# Copy this file to .env and customize as needed

# OpenAI API Configuration
OPENAI_API_KEY=your-openai-api-key-here
OPENAI_MODEL=gpt-4o-mini

# PostgreSQL
POSTGRES_USER=aibasic
POSTGRES_PASSWORD=aibasic123
POSTGRES_DB=aibasic_db

# MySQL
MYSQL_ROOT_PASSWORD=root123
MYSQL_DATABASE=aibasic_db
MYSQL_USER=aibasic
MYSQL_PASSWORD=aibasic123

# MongoDB
MONGO_INITDB_ROOT_USERNAME=aibasic
MONGO_INITDB_ROOT_PASSWORD=aibasic123
MONGO_INITDB_DATABASE=aibasic_db

# Redis
REDIS_PASSWORD=aibasic123

# RabbitMQ
RABBITMQ_DEFAULT_USER=aibasic
RABBITMQ_DEFAULT_PASS=aibasic123

# OpenSearch
OPENSEARCH_ADMIN_PASSWORD=Aibasic123!

# Vault
VAULT_DEV_ROOT_TOKEN_ID=aibasic-root-token

# MinIO
MINIO_ROOT_USER=aibasic
MINIO_ROOT_PASSWORD=aibasic123

# Application Settings
AIBASIC_DEBUG=false
AIBASIC_LOG_LEVEL=INFO
